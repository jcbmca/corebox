/*RenameDialog */

QWidget{
    background-color: @color02;
    color: @color07;
    border: none;}


QPushButton{
    background-color: transparent;
    color: @color07;
    border: 1px solid @color05;
    padding: 3px 5px 3px 5px;}
QPushButton:hover {
    background-color: @color04;}


QLineEdit{
    background-color: transparent;
    color: @color07;
    border: 1px solid @color05;
    padding: 5px 5px 5px 10px;}
