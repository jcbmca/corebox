/*SessionSaveDialog */

QWidget{
    background-color: @color02;
    color: @color07;
    border: none;}


QToolButton{
    background-color: transparent;
    color: @color07;
    border: 1px solid @color05;
    padding: 2px 3px 2px 3px;}
QToolButton:pressed{
    background-color: @color04;}
QToolButton:hover {
    background-color: @color04;}


QLineEdit{
    background-color: transparent;
    color: @color07;
    border: 1px solid @color05;
    padding: 5px 5px 5px 10px;}
